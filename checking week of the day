
#23 checking week of the day
x=int(input("enter your number of the day:"))
if(x==1):
    print("sunday")
elif(x==2):
    print("monday")
elif(x==3):
    print("tuesday")
elif(x==4):
    print("wednesday")
elif(x==5):
    print("thursday")
elif(x==6):
    print("friday")
elif(x==7):
    print("saturday")
else:
    print("invalid number")
