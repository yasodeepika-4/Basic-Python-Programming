#15find the longest of three numbers
a=int(input("enter the value:"))
b=int(input("enter the value:"))
c=int(input("enter the value:"))
d=max(a,b,c)
print("largest number:",d)

#16print arthemetic operations
a=10
b=20
print("addition of 2 numbers:",a+b)
print("subtraction of 2 numbers:",a-b)
print("multiplication of 2 numbers:",a*b)
print("division of 2 numbers:",a/b)


#17logical operation print numbers +,-,0
a=int(input("enter the number"))
if(a>0):
    print("positive")
elif(a==0):
    print("zero")
else:
    print("negative")


#18to find odd or even
a=int(input("enter the number"))
if (a%2==0):
  print("even number")
else:
  print("odd number")

#19
num=int(input("enter a number:"))
if(num%5==0 and num%11==0):
    print("number divisible by 5 and 11")
else:
    print("number is not divisible by 5 and 11")

#20
theory=int(input("enter the number:"))
practical=int(input("enter the number:"))
total=theory+practical
if theory>=35 and practical>=15 and total>60:
    print("student pass")
else:
    print("student failed")
    
    
