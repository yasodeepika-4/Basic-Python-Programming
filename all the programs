#6get input from user swap 2 numbers without 3rd variable
a=int(input("enter first num:"))
b=int(input("enter second num:"))
a=a+b;
b=a-b;
a=a-b;
print("a:",a)
print("b:",b)

#7swap 2 numbers using 3rd variable
a=10;
b=20;
temp=a
a=b
b=temp
print("after swapping a is:",a)
print("after swapping b is:",b)

# 8print primitive data types int,float,boolean
a=10
b=10.5
c=True
print("int:",a)
print("float:",b)
print("boolean:",c)

#9print non primitive data types list,tuple,set,dictionary.
my_list=[10,20,30,40,50]
my_tuple=(1,2,3,4,5)
my_set={100,200,300,400,500}
my_dict={"apple":20,"mangoes":40,"graps":60}
print("list:",my_list)
print("tuple:",my_tuple)
print("set:",my_set)
print("dictionary:",my_dict)

#10find the sg of a number 
a=int(input("enter the value:"))
sq=a*a
print("square:",sq)

#11avg of 3 numbers
a=int(input("enter the first number:"))
b=int(input("enter the second number:"))
c=int(input("enter the third number:"))
avg=(a+b+c)/2
print("avg of 3 numbers:",avg)

